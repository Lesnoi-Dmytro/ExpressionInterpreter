<AnchorPane prefWidth="400.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.course.CopyController">
   <children>
      <Button mnemonicParsing="false" onAction="#onCloseClick" prefHeight="25.0" prefWidth="120.0" text="Close" AnchorPane.bottomAnchor="65.0" AnchorPane.rightAnchor="30.0" />
