      <TextField fx:id="variable" prefHeight="25.0" prefWidth="120.0" text="x" AnchorPane.bottomAnchor="100.0" AnchorPane.rightAnchor="30.0" />
      <TextArea fx:id="function" editable="false" prefHeight="25.0" prefWidth="200.0" AnchorPane.bottomAnchor="135.0" AnchorPane.leftAnchor="30.0" AnchorPane.rightAnchor="30.0" AnchorPane.topAnchor="30.0" />
