      <Label prefHeight="25.0" text="name" AnchorPane.bottomAnchor="100.0" AnchorPane.leftAnchor="30.0" />
      <Label prefHeight="25.0" text="variable" AnchorPane.bottomAnchor="100.0" AnchorPane.rightAnchor="155.0" />
